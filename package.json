{
  "name": "arclight",
  "version": "0.9.7",
  "scripts": {
    "ng": "ng",
    "copy:config:xosp-dev:config": "npx cpy-cli dev-config/xosp/config.xosp-dev.json dev-static/config/ --flat --rename=config.json",
    "copy:config:xosp-dev:branding": "npx cpy-cli dev-config/xosp/plxtra-logo.svg dev-static/config/branding/images --flat",
    "copy:config:xosp-dev": "npm run copy:config:xosp-dev:config && npm run copy:config:xosp-dev:branding",
    "start:xosp-dev": "npm run copy:config:xosp-dev && ng serve",
    "copy:config:cfx_test-dev:config": "npx cpy-cli dev-config/config.cfx_demo-dev.json dev-static/config/ --flat --rename=config.json",
    "copy:config:cfx_test-dev:branding": "npx cpy-cli dev-config/branding/cf-markets/share-match-logo-version-1-01.png dev-static/config --flat",
    "copy:config:cfx_test-dev": "npm run copy:config:cfx_test-dev:config && npm run copy:config:cfx_test-dev:branding",
    "start:cfx_test-dev": "npm run copy:config:cfx_test-dev && ng serve",
    "build": "ng build",
    "build:docker": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "clean": "npm cache clean --force",
    "showlinks": "npm ls -g --depth=0",
    "link-core": "npm link @pbkware/js-utils revgrid @plxtra/motif-core",
    "unlink-core": "npm unlink --no-save @pbkware/js-utils revgrid @plxtra/motif-core"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.0",
    "@angular/cdk": "^19.2.1",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/platform-server": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/service-worker": "^19.2.0",
    "@ionic/angular": "^8.4.3",
    "@ionic/storage-angular": "^4.0.0",
    "@pbkware/js-utils": "^0.8.1",
    "@plxtra/motif-core": "^0.9.4",
    "angular-eslint": "^19.8.0",
    "date-fns": "^4.1.0",
    "decimal.js-light": "^2.5.1",
    "hacktimer": "^1.1.3",
    "highcharts": "^11.1.0",
    "locale-codes": "^1.3.1",
    "mobile-detect": "^1.4.5",
    "oidc-client-ts": "^3.1.0",
    "revgrid": "^0.9.2",
    "rollbar": "^2.26.4",
    "rxjs": "~7.8.1",
    "strongly-typed-events": "^3.0.11",
    "ts-md5": "^1.3.1",
    "tslib": "^2.5.0",
    "uuid": "^11.1.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.0",
    "@angular-eslint/builder": "^19.8.1",
    "@angular-eslint/eslint-plugin": "^19.8.1",
    "@angular-eslint/eslint-plugin-template": "^19.8.1",
    "@angular-eslint/schematics": "^19.8.1",
    "@angular-eslint/template-parser": "^19.8.1",
    "@angular/cli": "^19.2.0",
    "@angular/compiler-cli": "^19.2.0",
    "@ionic/angular-toolkit": "^12.1.1",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.29.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import-x": "^4.15.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.34.1"
  }
}
